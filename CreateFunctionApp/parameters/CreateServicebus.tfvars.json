# terraform.tfvars

function_app_name                 = "my-function-app"
location                          = "West Europe"
resource_group_name               = "my-resource-group"
app_service_plan_id               = "/subscriptions/your_subscription_id/resourceGroups/my-resource-group/providers/Microsoft.Web/serverfarms/my-app-service-plan"
storage_account_name              = "mystorageaccount"
storage_account_resource_group_name = "my-resource-group"
app_insights_name                 = "my-app-insights"
app_insights_resource_group_name = "my-resource-group"
runtime                           = "python"
runtime_version                   = "3.9"
https_only                         = true
always_on                         = false
tags = {
  environment = "dev"
  project     = "function-app"
}
private_endpoint_subnet_id = "/subscriptions/your_subscription_id/resourceGroups/my-vnet-rg/providers/Microsoft.Network/virtualNetworks/my-vnet/subnets/private-endpoint-subnet"
private_dns_zone_id = "/subscriptions/your_subscription_id/resourceGroups/my-dns-rg/providers/Microsoft.Network/privateDnsZones/privatelink.azurewebsites.net"